{
  "name": "ShowcaseII",
  "version": "1.0.0",
  "description": "Basic Thin2 SPA structure :)",
  "main": "main.js",
  "scripts": {
    "****** CLEAN ******": "",
    "clean": "npm run clean:test && npm run clean:compiled",
    "clean:all": "npm run clean && npm run clean:node_modules",
    "clean:aot": "rimraf src/**/*.ngsummary.json && rimraf src/**/*.metadata.json && rimraf src/**/*.js && rimraf src/**/*.js.map",
    "clean:npm-cache": "npm cache clean",
    "clean:test": "rimraf coverage",
    "clean:public": "rimraf ./dist/*",
    "clean:cordova": "rimraf cordova",
    "clean:electron": "rimraf dist && rimraf asar",
    "clean:compiled": "npm run clean:public && npm run clean:electron && npm run clean:cordova",
    "clean:translations": "rimraf src/translations/messages.xlf",
    "clean:node_modules": "rimraf node_modules",
    "****** TEST ******": "",
    "test": "npm run clean:test && npm run test:karma",
    "test:karma": "karma start ./karma/karma.config.js",
    "test:production": "node staticServer.js",
    "****** CORDOVA BUILD ******": "",
    "create:cordova": "cordova create cordova sh.surge.ngtaskmanager",
    "compile:cordova": "cross-env NODE_ENV=cordova webpack --progress --bail --profile",
    "provide:cordova": "rimraf cordova/config.xml && cp config.xml cordova && rimraf cordova/www/* && cp -r dist/* cordova/www && rimraf dist",
    "android:cordova": "cd cordova && cordova platform add android && cordova build android",
    "browser:cordova": "cd cordova && cordova platform add browser",
    "package:cordova": "npm run provide:cordova && npm run android:cordova",
    "build:cordova": "npm run clean && npm test && npm run create:cordova && npm run compile:cordova && npm run package:cordova",
    "build:cordovaWT": "npm run clean && npm run create:cordova && npm run compile:cordova && npm run package:cordova",
    "****** ELECTRON BUILD ******": "",
    "tsc:electron": "tsc --outDir dist/ main.electron.ts && mv dist/main.electron.js dist/main.js",
    "compile:electron": "node ./build/production-electron-build.js",
    "package:electron": "asar pack dist ./asar/app.asar",
    "build:electron": "npm run clean && npm test && npm run tsc:electron && npm run compile:electron && npm run package:electron",
    "build:electronWT": "npm run clean && npm run tsc:electron && npm run compile:electron && npm run package:electron",
    "****** i18n ******": "",
    "i18n:extract": "npm run clean:translations && rimraf xliffmerge.conf.json && node ./build/extract-translations.js && npm run i18n:merge",
    "i18n:merge": "xliffmerge --profile xliffmerge.conf.json",
    "****** WEB BUILD ******": "",
    "installdeps": "npm install --only=dev && npm install --production && npm run build:production && rimraf ./.local-chromium",
    "build:dev": "npm run clean && npm test && node ./build/development-build.js",
    "build:production": "npm run clean && npm test && node ./build/production-aot-build.js",
    "****** RUNNING ******": "",
    "start": "webpack-dev-server --config config/webpack.local.js --inline --port 3100 --watch --content-base src/",
    "cordova": "cd cordova && cordova run browser",
    "cordova:android": "cd cordova && cordova run android -lc",
    "electron": "electron dist",
    "****** POSTINSTALL ******": "",
    "postinstall": "node chromiumInstall.js"
  },
  "author": "BBVA",
  "chromium_revision": "514418",
